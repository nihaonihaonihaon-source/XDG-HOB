    PremiumOnly = false
})
local Section = Tab:AddSection({	Name = "地瓜创作"})
Tab:AddSlider({
	Name = "移动速度",
	Min = 16,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "数值",
	Callback = function(Value)
		if game.Players.LocalPlayer.Character then	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
		end
	end    
})
Tab:AddSlider({
	Name = "跳跃",
	Min = 50,
	Max = 200,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "数值",
	Callback = function(Value)
		if game.Players.LocalPlayer.Character then	game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
		end
	end    
})
Tab:AddTextbox({
	Name = "跳跃高度设置",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		if game.Players.LocalPlayer.Character then	game.Players.LocalPlayer.Character.Humanoid.JumpPower = tonumber(Value)
		end
	end
})
